@model IEnumerable<newBugTracker.Models.TicketNotification>


<div class="row">
    <div class="col-md-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                <h3 class="panel-title">Responsive example</h3>
            </div>
            <div class="panel-body">
                <table id="datatable-responsive" class="table table-striped table-bordered dt-responsive nowrap dataTable " cellspacing="0" width="100%">
                    <thead>
                        <tr>
                            <th>Ticket Title</th>
                            <th>Project Name</th>
                            <th>Notification Message</th>
                            <th>Notification Created</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>@item.Ticket.Title</td>
                                <td>@item.Ticket.Project.Name</td>
                                <td>@Html.Raw(item.Message)</td>
                                <td>@item.Created</td>
                            </tr>
                        }

                    </tbody>
                </table>
            </div>
        </div>
    </div>

</div> <!-- End Row -->
<p>
    <input action="action" onclick="window.history.go(-1); return false;" type="button" value="Back" class="btn btn-inverse btn-sm m-b-5" />
</p>
